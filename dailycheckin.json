            "name": "手动更新",
            "type": "schedule",
            "time": "0",
            "job": {
                "type": "exec",
                "target": "git checkout master && git fetch --all && git reset --hard origin/master && git pull -cwd script/Shell/checkinpanel"
            },
            "running": false
